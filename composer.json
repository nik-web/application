{
    "name": "nik-web/application",
    "description": "Laminas MVC Application",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mvc",
        "base",
        "application",
        "framework"
    ],
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Contact\\": "module/Contact/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "ContactTest\\": "module/Contact/test/"
        }
    },
    "authors": [
        {
            "name": "Niklaus HÃ¶pfner",
            "email": "editor@nik-web.net"
        }
    ],
    "require-dev": {
        "laminas/laminas-component-installer": "^3.3",
        "laminas/laminas-test": "^4.8"
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-component-installer": true
        }
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "laminas/laminas-mvc": "^3.6",
        "laminas/laminas-development-mode": "^3.11",
        "laminas/laminas-navigation": "^2.18",
        "laminas/laminas-mvc-form": "^2.1",
        "laminas/laminas-mvc-i18n": "^1.7",
        "laminas/laminas-math": "^3.6",
        "laminas/laminas-session": "^2.16",
        "laminas/laminas-text": "^2.10",
        "laminas/laminas-captcha": "^2.16",
        "laminas/laminas-mail": "^2.23",
        "laminas/laminas-mvc-plugin-flashmessenger": "^1.9"
    }
}
